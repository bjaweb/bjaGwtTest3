<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.bja.bapps.gwt.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  
  <servlet>
    <servlet-name>exempleServlet</servlet-name>
    <servlet-class>com.bja.bapps.gwt.server.ExempleServiceImpl</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/bjagwttest3/greet</url-pattern>
  </servlet-mapping>
  
  
  <servlet-mapping>
    <servlet-name>exempleServlet</servlet-name>
    <url-pattern>/bjagwttest3/exampleservice</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
    <servlet-name>operatorServlet</servlet-name>
    <servlet-class>com.bja.bapps.gwt.server.OperatorServiceImpl</servlet-class>
  </servlet>
            
   <servlet-mapping>
    <servlet-name>operatorServlet</servlet-name>
    <url-pattern>/bjagwttest3/operator</url-pattern>
  </servlet-mapping>
  
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>BjaGwtTest3.html</welcome-file>
  </welcome-file-list>

</web-app>
